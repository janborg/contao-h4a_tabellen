{
    "name":"janborg/contao-h4a_tabellen",
    "description":"Tabellen, Spielpläne und Ergebnisse über die Handball4all json API als Contao Inhaltselemente ausgeben",
    "keywords":["Handball","H4a","Ergebnisse","Spielplan","Tabelle"],
    "type":"contao-bundle",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Jan Lünborg",
            "homepage":""
        }
    ],
    "require":{
        "php":"^5.6|^7.1",
        "contao/core-bundle":"^4.4",
        "codefog/contao-haste": "^4.17"
    },
    "require-dev": {
         "contao/manager-plugin": "^2.0"
    },
    "conflict": {
         "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Janborg\\H4aTabellen\\": "src/"
          }
      },
    "extra":{
      "contao-manager-plugin": "Janborg\\H4aTabellen\\ContaoManager\\Plugin"
    }
}
