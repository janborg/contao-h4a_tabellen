{
    "name":"janborg/contao-h4a_tabellen",
    "description":"Tabellen, Spielpläne und Ergebnisse über die Handball4all json API als Contao Inhaltselemente ausgeben",
    "keywords":["Handball","H4a","Ergebnisse","Spielplan","Tabelle"],
    "type":"contao-bundle",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Jan Lünborg",
            "homepage":""
        }
    ],
    "require":{
        "php":"^5.6|^7.1",
        "contao/core-bundle":"~4.4"
    },
    "require-dev": {
         "contao/manager-plugin": "^2.0"
    },
    "conflict": {
         "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Janborg\\H4aTabellenBundle\\": "src/"
          },
          "classmap": [
              "src/Resources/contao/"
              ],
          "exclude-from-classmap": [
              "src/Resources/contao/config/",
              "src/Resources/contao/dca/",
              "src/Resources/contao/languages/",
              "src/Resources/contao/templates/"
            ]
      },
    "extra":{
      "contao-manager-plugin": "Janborg\\H4aTabellenBundle\\ContaoManager\\Plugin"
    }
}
